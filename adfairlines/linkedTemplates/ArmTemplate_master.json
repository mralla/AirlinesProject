{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfairlines"
		},
		"LS_ABS_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "absairlines-cs"
		},
		"LS_ADLSG2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsairlines.dfs.core.windows.net"
		},
		"LS_AKV_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://akvairlines.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfairlines_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AKV_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_AKV_properties_typeProperties_baseUrl')]"
					},
					"LS_ABS_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_ABS_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_ADLSG2_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLSG2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}